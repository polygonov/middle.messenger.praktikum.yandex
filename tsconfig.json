{
  "compileOnSave": true,
  "compilerOptions": {
    "outDir": "dist",
    "target": "es5",
    "declaration": false,
    "module": "commonjs",
    "sourceMap": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "moduleResolution": "node",
    "skipLibCheck": true,
    "downlevelIteration": true,
    "esModuleInterop": true
  }
}
